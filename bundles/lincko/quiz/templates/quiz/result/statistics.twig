{% extends '/bundles/lincko/quiz/templates/base.twig' %}
{% block frame %}
	<div id="begin_quiz_result_statistics" class="visibility_hidden">
		<link rel="stylesheet" href="{{ _filelatest('/lincko/quiz/styles/quiz/result/statistics.css') }}" type="text/css" />
		<script src="{{ _filelatest('/lincko/quiz/scripts/quiz/result/statistics.js') }}" type="text/javascript"></script>
	</div>

	<div class="quiz_result_statistics_top">
		<div>
			{{ _trans('quiz', 0, 11, 'html') }}{# Thank you for your participation üëç ! #}
		</div>
		<img id="quiz_result_statistics_gif" src="{{ _filelatest('/lincko/quiz/images/gif/thankyou02.gif') }}" />
	</div>

	<div id="quiz_result_statistics_scan">
		<span id="quiz_result_statistics_scan_sentence">{{ _trans('quiz', 0, 15, 'html') }}{# Scan next quiz #}</span>
		<img id="quiz_result_statistics_scan_picture" src="{{ _filelatest('/lincko/quiz/images/phone.png') }}" />
	</div>

	<div class="quiz_result_statistics_followus">
		<div>{{ _trans('quiz', 0, 4, 'html') }}{# Follow us on WeChat #}</div>
		<img class="quiz_result_statistics_picture" src="{{ _filelatest('/lincko/wrapper/images/wechat/official.jpg') }}" />
	</div>

	

	<div id="end_quiz_result_statistics" class="visibility_hidden"></div>
{% endblock %}
